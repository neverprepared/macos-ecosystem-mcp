# macOS MCP Server Configuration

# Logging level: debug, info, warn, error
LOG_LEVEL=info

# Script execution timeout (milliseconds)
SCRIPT_TIMEOUT=30000

# Enable security validation (should always be true in production)
ENABLE_SECURITY_VALIDATION=true

# Default reminder list name
DEFAULT_REMINDER_LIST=Reminders

# Default calendar name
DEFAULT_CALENDAR=Calendar

# Default notes folder
DEFAULT_NOTES_FOLDER=Notes
