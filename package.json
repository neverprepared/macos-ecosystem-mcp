{
  "name": "macos-ecosystem-mcp",
  "version": "0.1.0",
  "description": "Secure, semantic MCP server for macOS application ecosystem (Reminders, Calendar, Notes, etc.)",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "macos-mcp": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "macos",
    "applescript",
    "reminders",
    "calendar",
    "notes",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "zod": "^3.25.0",
    "run-applescript": "^7.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "tsx": "^4.21.0",
    "@types/node": "^22.10.5",
    "vitest": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.18.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "prettier": "^3.4.2"
  }
}
